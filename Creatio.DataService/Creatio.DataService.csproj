<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Kirill Krylov, CPA</Authors>
    <Company>Creatio</Company>
    <Description>Allows CRUD operation via DataService</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageIcon>Accelerate.png</PackageIcon>
    <PackageIconUrl />
    <RepositoryUrl>https://github.com/kirillkrylov/Creatio.DataService/blob/master/README.md</RepositoryUrl>
    <PackageTags>Creatio, DataService</PackageTags>
    <AssemblyVersion>1.0.1.0</AssemblyVersion>
    <FileVersion>1.0.1.0</FileVersion>
    <NeutralLanguage>aa-ET</NeutralLanguage>
    <Version>1.0.3</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="HtmlAgilityPack" Version="1.11.17" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
  </ItemGroup>
  <ItemGroup>
    <Content Include="readme.txt">
      <Pack>true</Pack>
      <PackagePath>\</PackagePath>
    </Content>
  </ItemGroup>
  <ItemGroup>
    <None Include="..\..\..\..\Pictures\Accelerate.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
